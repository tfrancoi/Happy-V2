(
	    [# begin stdp.ha #]

	

(fun *s (of a b r) 
	(if (= 0 a) 
		(return r)
		(return (*s (- a 1) b (+ r b)))
	)
)
	

[# end stdp.ha #]

	
	
	
	(fun > (of a b) (return (< b a)))
	(fun >= (of a b) 
		(if (= a b) 
			(return 1)
			(return (< b a))
		)
	)
	
	(fun <= (of a b)  
		(if (= a b) 
			(return 1)
			(return (< a b))
		)
	)
	
	(fun * (of a b) (return (*s a b 0)))
	
	
)
[# end std.ha #]

(	
